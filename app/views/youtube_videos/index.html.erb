<h1>Listing youtube_videos</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Yt video embed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @youtube_videos.each do |youtube_video| %>
  <tr>
    <td><%= youtube_video.title %></td>
    <td><%= youtube_video.description %></td>
    <td><%= youtube_video.yt_video_embed %></td>
    <td><%= link_to 'Show', youtube_video %></td>
    <td><%= link_to 'Edit', edit_youtube_video_path(youtube_video) %></td>
    <td><%= link_to 'Destroy', youtube_video, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Youtube video', new_youtube_video_path %>
